function convertrestore(line){
nrestore="";

ret=split (line,a,":=");
ret=split (a[2],c,"restore");
if (ret>1){
gsub(/\ /,"",c[1]);
if (c[1]!=""){
return (line);
}}

ret=split (c[2],d,"(");
if (ret>1){
gsub(/\ /,"",d[1]);
if (d[1]!=""){
return (line);
}}

ret=split (line,a,":=");
if (ret<2){
return (line);}
return_value=a[1];
ret=split(a[2],b,";");
arguments=b[1];
ret=split(arguments,a,",");
if (ret>2){
return (line);}

arg1=a[1];
arg2=a[2];
gsub(/\(/,"",arg1);
gsub(/restore/,"",arg1);
gsub(/\)/,"",arg2);


 	 	      nrestore=return_value" := restore ("arg2"+\".\"+"arg1",null);\n";
			  return (nrestore);
}

function convertstore(line){
nstore="";
ret =split(line,a,"(");
if (ret<2){
return (line);}
ret=split (line,a,"store");
if (ret>1){
gsub(/\ /,"",a[1]);
if (a[1]!=""){
return (line);
}}

ret=split (a[2],b,"(");
if (ret>1){
gsub(/\ /,"",b[1]);
if (b[1]!=""){
return (line);
}}
ret=split(line,a,"(");
ret=split(a[2],b,";");
argument=b[1];
ret=split(argument,a,",");
if (ret>1){
return(line);}

gsub(/\)/,"",argument);
                       gsub(/\ /,"",argument);
 	 	      nstore="store ("argument","argument".nameidx+\".\"+"argument".zoneidx,TRUE);\n";

			  return (nstore);
}

