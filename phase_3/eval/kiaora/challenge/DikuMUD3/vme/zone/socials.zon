/*arm punch */
dilbegin armpunch(arg:string);
external
  socialact
(act_s:string,act_i:integer,act_1:unitptr,act_2:unitptr,act_3:unitptr,act_to:integer);

var
vict:unitptr;
code
{
if (arg!="")
{
vict:=findunit(self,arg,SOCIAL_FIND,null);
if ((vict==null) or not visible(self,vict))
{
   sendtext ("<div class='social_self'>Sniff! Your friend isn't here.:(</div><br/>",self);
quit;
}
if ((vict.type!=UNIT_ST_PC) and (vict.type!=UNIT_ST_NPC))
{
   sendtext ("<div class='social_self'>Sniff! Your friend isn't here.:(</div><br/>",self);
quit;
}
if (vict.position==POSITION_DEAD)
{
socialact("<div class='social_self'>Too late to be friends with $3m now. </div>",
       A_HIDEINV,self,null,vict,TO_CHAR);
act ("<div class='social_other'>$1n gives the corpse of $3n a punch in the arm. " +
      "There seems to be something odd about $1n.</div>",
       A_HIDEINV,self,null,vict,TO_NOTVICT);


quit;
}
if (vict.position == POSITION_SLEEPING)
{
socialact("<div class='social_self'>You give $3n a friendly punch in the arm.</div>",
      A_ALWAYS,self,null,vict,TO_CHAR);
socialact("<div class='social_other'>$1n gives $3n a friendly punch in the arm to "+
      "wake $3m.  </div>",
        A_HIDEINV,self,null,vict,TO_NOTVICT);
socialact("<div class='social_other'>You dream someone is using your arm for " +
      "a punching bag.</div>",
        A_HIDEINV,self,null,vict,TO_VICT);
quit;
}

if (self==vict)
{
socialact("<div class='social_self'>You punch yourself in the arm. At least " +
      "SOMEONE is glad to see you. </div>",
        A_HIDEINV,self,null,null,TO_CHAR);
socialact("<div class='social_other'>$1n punches $1mself in the arm. " +
      "Seems $1n is $1s own best friend. </div>",
        A_HIDEINV,self,null,null,TO_REST);
  send_done ("armpunch",self,null,vict,0,"",null);
quit;
}
socialact("<div class='social_self'>You give $3n a comradely tap on the arm. " +
      "You two are best friends! </div>",
        A_HIDEINV,self,null,vict,TO_CHAR);
socialact("<div class='social_other'>$1n gives $3n a friendly punch in the arm. </div>",
        A_HIDEINV,self,null,vict,TO_NOTVICT);
socialact("<div class='social_other'>$1n punches you in the arm. Oww! "+
      "$1n is stronger than $1e looks! </div>",          //corrected reference
        A_HIDEINV,self,null,vict,TO_VICT);
  send_done ("armpunch",self,null,vict,0,"",null);
quit;
}

socialact("<div class='social_self'>You give your invisible friend a friendly "  +
      "sock in the arm. </div>",
       A_HIDEINV,self,null,null,TO_CHAR);
socialact("<div class='social_other'>$1n gives the air a friendly punch," +
      "maybe $1e needs professional help? </div>",
       A_HIDEINV,self,null,null,TO_REST);
  send_done ("armpunch",self,null,null,0,"",null);
quit;
}
dilend

